.btn {
  position: absolute;
  top: 50px;
  left: 50%;
  transform: translateX(-50%);
  transition: margin 1s;
}

.click-btn {
  margin-top: 200px;
}

.is-move {
  margin-top: -60px;
}

.title {
  position: fixed;
  top: 0;
  left: 50%;
  width: 358rpx;
  height: 200rpx;
  background-color: pink;
  transform: translate(-50%);
}