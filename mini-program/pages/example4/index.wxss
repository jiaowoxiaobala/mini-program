/* pages/example4/index.wxss */
scroll-view:nth-child(2) {
  margin: 40rpx 0;
  white-space: nowrap
}
scroll-view:nth-child(2) .scroll-view-item {
  display: inline-block;
  margin-right: 30rpx;
  width: 200rpx;
  height: 200rpx;
  background: #535d95;
}
scroll-view:last-child {
  height: 600rpx;
}
scroll-view:last-child .scroll-view-item {
  margin-bottom: 30rpx;
  width: 200rpx;
  height: 200rpx;
  background: #535d95;
}